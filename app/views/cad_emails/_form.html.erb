<%= form_for @cad_email, :html => { :class => "form-horizontal cad_email" } do |f| %>

  <% if @cad_email.errors.any? %>
    <div id="error_expl" class="panel panel-danger">
      <div class="panel-heading">
        <h3 class="panel-title"><%= pluralize(@cad_email.errors.count, "error") %> prohibited this cad_email from being saved:</h3>
      </div>
      <div class="panel-body">
        <ul>
        <% @cad_email.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
        </ul>
      </div>
    </div>
  <% end %>

  <div class="form-group">
    <%= f.label :email, :class => 'control-label col-lg-2' %>
    <div class="col-lg-4">
      <%= f.text_field :email, :class => 'form-control' %>
    </div>
    <%=f.error_span(:email) %>
  </div>
  <div class="form-group">
    <%= f.label :senha_email, :class => 'control-label col-lg-2' %>
    <div class="col-lg-2">
      <%= f.text_field :senha_email, :class => 'form-control' %>
    </div>
    <%=f.error_span(:senha_email) %>
  </div>
  <div class="form-group">
    <%= f.label :skype, :class => 'control-label col-lg-2' %>
    <div class="col-lg-4">
      <%= f.text_field :skype, :class => 'form-control' %>
    </div>
    <%=f.error_span(:skype) %>
  </div>
  <div class="form-group">
    <%= f.label :senha_skype, :class => 'control-label col-lg-2' %>
    <div class="col-lg-2">
      <%= f.text_field :senha_skype, :class => 'form-control' %>
    </div>
    <%=f.error_span(:senha_skype) %>
  </div>

  <div class="form-group">
    <div class="col-lg-offset-2 col-lg-10">
      <%= f.submit 'Salvar', data: { disable_with: "Processando..." }, :class => 'btn btn-primary' %>
      <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                cad_emails_path, :class => 'btn btn-default' %>
    </div>
  </div>

<% end %>
