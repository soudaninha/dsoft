<%- model_class = CadEmail -%>
<div class="page-header">
  <h1>Email / Skype</h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:email) %>:</strong></dt>
  <dd><%= @cad_email.email %></dd>
  <dt><strong><%= model_class.human_attribute_name(:senha_email) %>:</strong></dt>
  <dd><%= @cad_email.senha_email %></dd>
  <dt><strong><%= model_class.human_attribute_name(:skype) %>:</strong></dt>
  <dd><%= @cad_email.skype %></dd>
  <dt><strong><%= model_class.human_attribute_name(:senha_skype) %>:</strong></dt>
  <dd><%= @cad_email.senha_skype %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              cad_emails_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_cad_email_path(@cad_email), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              cad_email_path(@cad_email),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
