<%- model_class = DnsServer -%>
<div class="page-header">
  <h1>Servidor DNS</h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name("email") %>:</strong></dt>
  <dd><%= @dns_server.email %></dd>
  <dt><strong><%= model_class.human_attribute_name("senha") %>:</strong></dt>
  <dd><%= @dns_server.password %></dd>
  <dt><strong><%= model_class.human_attribute_name("servidor DNS") %>:</strong></dt>
  <dd><%= @dns_server.dns %></dd>
  <dt><strong><%= model_class.human_attribute_name("cliente") %>:</strong></dt>
  <dd><%= @dns_server.client.name %></dd>
</dl>

<%= link_to "Voltar",
              dns_servers_path, :class => 'btn btn-default'  %>
<%= link_to "Editar",
              edit_dns_server_path(@dns_server), :class => 'btn btn-default' %>
<%= link_to "Excluir",
              dns_server_path(@dns_server),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
